   },
   "outputs": [],
   "source": [
    "def compute_online_counter_numpy(categories_stream):\n",
    "    occurences = np.bincount(categories_stream)\n",
    "    before = occurences.cumsum() - occurences\n",
    "    positions = categories_stream.argsort(kind='mergesort').argsort(kind='mergesort')\n",
    "    return positions - before[categories_stream]"
   ]
  },
