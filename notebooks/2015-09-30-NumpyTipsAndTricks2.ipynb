    "import pandas\n",
    "df = pandas.DataFrame({'cat':  categories_stream, 'ones': np.ones(len(categories_stream))})\n",
    "# truncating to first 10000, otherwise it will never finish\n",
    "df = df[:10000]\n",
    "%time result = df.groupby('cat')['ones'].transform(pandas.Series.cumsum)"
   ]
